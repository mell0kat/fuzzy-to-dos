  "compilerOptions": {
    "sourceMap": true,
    "noImplicitAny": true,
    "lib": ["dom", "esnext"],
    "moduleResolution": "node",
    "module": "esnext",
    "target": "es6",
    "jsx": "react",
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true
  },
  "exclude": ["node_modules"]
}
